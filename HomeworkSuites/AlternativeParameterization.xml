<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="AlternativeParameterizationSuite">
    <test name="Test1">
        <parameter name="firstName" value="firstName1" />
        <parameter name="lastName" value="lastName1" />
        <parameter name="gender" value="Male" />
        <parameter name="mobileNumber" value="111111111" />
        <classes>
            <class name="ge.tbc.itacademy.Tests.ParameterizationTests.AlternativeParameterization"/>
        </classes>
    </test>
    <test name="Test2">
        <parameter name="firstName" value="firstName2" />
        <parameter name="lastName" value="lastName2" />
        <parameter name="gender" value="Female" />
        <parameter name="mobileNumber" value="22222222" />
        <classes>
            <class name="ge.tbc.itacademy.Tests.ParameterizationTests.AlternativeParameterization"/>
        </classes>
    </test>
    <test name="Test3">
        <parameter name="firstName" value="firstName3" />
        <parameter name="lastName" value="lastName3" />
        <parameter name="gender" value="Other" />
        <parameter name="mobileNumber" value="333333333" />
        <classes>
            <class name="ge.tbc.itacademy.Tests.ParameterizationTests.AlternativeParameterization"/>
        </classes>
    </test>

</suite>